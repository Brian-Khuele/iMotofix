{
  "name": "hydra",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist/src",
    "start:dev": "nodemon --config nodemon.json",
    "test": "nodemon --config nodemon.test.json",
    "clean": "concurrently  \"knex migrate:rollback\"  \"knex migrate:latest\"  \"knex seed:run\""
  },
  "_moduleAliases": {
    "src": "dist/src",
    "controllers": "dist/src/controllers",
    "routes": "dist/src/routes",
    "shared": "dist/src/shared",
    "db": "dist/db"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "knex": "^0.21.12",
    "knex-migrate": "^1.7.4",
    "mailgen": "^2.0.14",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.1",
    "pg": "^8.6.0",
    "tslib": "^1.14.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.45",
    "@types/jasmine": "^3.5.10",
    "@types/jsonfile": "^5.0.0",
    "@types/mailgen": "^2.0.4",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^13.13.36",
    "@types/nodemailer": "^6.4.2",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "find": "^0.3.0",
    "fs-extra": "^9.0.0",
    "jasmine": "^3.5.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
